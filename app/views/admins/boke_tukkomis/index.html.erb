
<div class="row">
  <div class="col-xs-10 col-xs-offset-1">
      <h2>ボケ・ツッコミ一覧</h2>

  <div class="text-right">
      <%= link_to "新規登録" ,new_admins_boke_tukkomi_path, class:"btn btn-lg btn-primary" %>
  </div>

      <!-- ofsetは左右の余白 -->
      <table class="table">
        <thead>
          <tr>
            <th>ストーリーID</th>
            <th>ストーリー名</th>
            <th>ジャンル</th>
            <th>登録日</th>
          </tr>
        </thead>

        <tbody>
          <% @boke_tukkomis.each do |boke_tukkomi| %>
            <tr>
              <td><%= boke_tukkomi.id %></td>
              <td><%= link_to boke_tukkomi.name ,new_admins_boke_tukkomi_path(boke_tukkomi) %></td>
              <td><%= boke_tukkomi.genre.id %></td>
              <td><%= boke_tukkomi.created_at %></td>
            </tr>
            <% end %>
        </tbody>
      </table>
    </div>
  </div>
